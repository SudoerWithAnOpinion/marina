{
  "name": "marina",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vavite-loader vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest unit",
    "test:integration": "vitest --single-thread integration",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "commit": "commit",
    "pipeline:set": "fly -t revenance set-pipeline --team marina --pipeline marina --config ./ci/concourse/pipeline.yaml  --load-vars-from ./ci/concourse/variables.private.yaml",
    "ci:test": "svelte-kit sync && vitest run",
    "platform:setup": "waypoint install -platform=docker -accept-tos; waypoint init",
    "waypoint:up": "waypoint up",
    "waypoint:down": "waypoint destroy --auto-approve"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/prompt-cli": "^17.4.4",
    "@histoire/plugin-svelte": "^0.12.4",
    "@lucia-auth/adapter-test": "^0.4.3",
    "@playwright/test": "^1.31.2",
    "@sveltejs/kit": "^1.11.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^3.2.0",
    "@types/node": "^18.14.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitest/coverage-c8": "^0.27.3",
    "autoprefixer": "^10.4.14",
    "bufferutil": "^4.0.7",
    "carbon-components-svelte": "^0.73.2",
    "carbon-icons-svelte": "^11.4.0",
    "carbon-preprocess-svelte": "^0.9.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-testing-library": "^5.10.2",
    "flowbite-svelte": "^0.29.13",
    "histoire": "^0.12.4",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "sass": "^1.58.3",
    "sequelize-cli": "^6.6.0",
    "svelte": "^3.55.1",
    "svelte-check": "^2.10.3",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "utf-8-validate": "^6.0.3",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  },
  "type": "module",
  "dependencies": {
    "@carbon/charts-svelte": "^1.6.14",
    "@lucia-auth/sveltekit": "^0.6.9",
    "@sveltejs/adapter-node": "^1.2.2",
    "@vavite/node-loader": "^1.6.1",
    "d3": "^7.8.2",
    "lodash": "^4.17.21",
    "lucia-auth": "^0.8.0",
    "luxon": "^3.3.0",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.29.2",
    "sqlite3": "^5.1.4",
    "ws": "^8.12.1",
    "youch": "^3.2.3",
    "youch-terminal": "^2.2.0",
    "zod": "^3.21.4"
  }
}
