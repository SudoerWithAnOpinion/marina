{
	"name": "marina",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"story:dev": "histoire dev",
		"story:build": "histoire build",
		"story:preview": "histoire preview",
		"prepare": "husky install",
		"lint-staged": "lint-staged",
		"commit": "commit",
		"pipeline:set": "fly -t revenance set-pipeline --team marina --pipeline marina --config ./ci/concourse/pipeline.yaml  --load-vars-from ./ci/concourse/variables.private.yaml",
		"ci:test": "svelte-kit sync && vitest run",
		"platform:setup": "waypoint install -platform=docker -accept-tos; waypoint init",
		"waypoint:up": "waypoint up",
		"waypoint:down": "waypoint destroy --auto-approve"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.4.3",
		"@commitlint/config-conventional": "^17.4.3",
		"@commitlint/prompt-cli": "^17.4.3",
		"@histoire/plugin-svelte": "^0.12.4",
		"@playwright/test": "^1.30.0",
		"@sveltejs/adapter-auto": "^1.0.3",
		"@sveltejs/kit": "^1.7.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.2",
		"@types/lodash": "^4.14.191",
		"@types/luxon": "^3.2.0",
		"@types/node": "^18.13.0",
		"@types/testing-library__jest-dom": "^5.14.5",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"@typescript-eslint/parser": "^5.52.0",
		"@vitest/coverage-c8": "^0.27.3",
		"autoprefixer": "^10.4.13",
		"carbon-icons-svelte": "^11.4.0",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"eslint-plugin-testing-library": "^5.10.2",
		"flowbite-svelte": "^0.29.13",
		"histoire": "^0.12.4",
		"husky": "^8.0.3",
		"jsdom": "^21.1.0",
		"lint-staged": "^13.1.2",
		"postcss": "^8.4.21",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"sequelize-cli": "^6.6.0",
		"svelte": "^3.55.1",
		"svelte-check": "^2.10.3",
		"tailwindcss": "^3.2.6",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.1",
		"vitest": "^0.25.8"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-node": "^1.2.0",
		"@trpc/client": "^10.11.1",
		"@trpc/server": "^10.11.1",
		"lodash": "^4.17.21",
		"luxon": "^3.2.1",
		"pg": "^8.9.0",
		"pg-hstore": "^2.3.4",
		"pnpm": "^7.27.0",
		"sequelize": "^6.28.0",
		"sqlite3": "^5.1.4",
		"trpc-sveltekit": "^3.3.0"
	}
}
